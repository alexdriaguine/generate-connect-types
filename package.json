{
  "name": "generate-connect-types",
  "version": "0.2.1",
  "main": "build/index.js",
  "bin": {
    "generate-connect-types": "build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "tsc -p tsconfig.json -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "yarn test && yarn compile"
  },
  "dependencies": {
    "typescript": "^2.8.3",
    "prettier": "^1.12.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/prettier": "^1.12.4",
    "@types/react-redux": "^6.0.0",
    "@types/redux": "^3.6.0",
    "jest": "^22.4.4",
    
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "ts-jest": "^22.4.6"
  },
  "keywords": [
    "typescript",
    "prop types",
    "react",
    "redux",
    "react-redux",
    "automatic generation"
  ],
  "author": "Alexandre Driaguine",
  "license": "MIT",
  "description": "",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
